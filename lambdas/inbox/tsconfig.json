{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["ES6"],
    "module": "commonjS",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ES6"
  },
  "exclude": ["node_modules", "dist"]
}
